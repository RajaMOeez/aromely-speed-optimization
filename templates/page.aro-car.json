n seek forward\n     * 2) Then jump to 5s. The 'timeupdate' listener will pause at exactly 5s once it's reached.\n     */\n    previewVideo.addEventListener('canplay', function() {\n      if (previewVideo.duration > 5) {\n        previewVideo.currentTime = 5;\n      }\n    });\n    // Once we hit time = 5, pause the video so it stays as a thumbnail\n    previewVideo.addEventListener('timeupdate', function() {\n      if (Math.floor(previewVideo.currentTime) === 5) {\n        previewVideo.pause();\n      }\n    });\n\n    // Open modal and play the modal video\n    function openVideoModal() {\n      modal.style.display = \"flex\"; // show the modal\n      modalVideo.play();\n      previewVideo.pause();  // ensure preview is paused\n    }\n\n    // Close modal and reset modal video\n    function closeVideoModal() {\n      modal.style.display = \"none\";\n      modalVideo.pause();\n      modalVideo.currentTime = 0;\n    }\n\n    // Event listeners for playing and closing\n    playButton.addEventListener(\"click\", openVideoModal);\n    previewVideo.addEventListener(\"click\", openVideoModal);\n    closeModal.addEventListener(\"click\", closeVideoModal);\n\n    // Clicking outside the modal also closes it\n    window.addEventListener(\"click\", function(event) {\n      if (event.target === modal) {\n        closeVideoModal();\n      }\n    });\n  });\n</script>\n\n<!-- End Custom Shopify Video + Title Section -->",
